    @if (user != null)
    {
        <script src="https://cdn.jsdelivr.net/npm/js-cookie@rc/dist/js.cookie.min.js"></script>
        <script src="~/Scripts/jquery.signalR-2.4.1.min.js"></script>
        <!--Reference the autogenerated SignalR hub script. -->
        <script src="~/signalr/hubs"></script>
        <!--SignalR script to update the chat page and send messages.-->
        <script>
            var hub
            $(document).ready(function () {
                // Reference the auto-generated proxy for the hub.
                hub = $.connection.notiHub;
                // Create a function that the hub can call back to display messages.
                hub.client.addNewMessageToPage = function (Noti, template, date) {
                    let templateMessage = '<a data-id="' + Noti.NotificationID + '" href="' + Noti.URL + '" class="navi-item';
                    if (!Noti.IsRead)
                        templateMessage += ' not-read-noti';
                    templateMessage += '"><div class="navi-link">' +
                        '<div class="navi-icon mr-2">' +
                        '<i class="' + Noti.Icon + '"></i>' +
                        '</div>' +
                        '<div class="navi-text">' +
                        '<div';
                    if (Noti.IsRead)
                        templateMessage += ' class="text-muted"'
                    templateMessage += '>' +
                        template +
                        '</div>' +
                        '<div class="text-muted">' +
                        date +
                        '</div>' +
                        '</div>' +
                        '</div>' +
                        '</a>';
                    if ($("#event-noti a").length == 0)
                        $("#event-noti").text("");
                    $("#event-noti").html(templateMessage + $("#event-noti").html());
                    $("#noti-number span").text(parseInt($("#noti-number span").text()) + 1);
                    $("#noti-number").show();
                    $("#noti-pulse").show();
                };

                $.connection.hub.start().done(function () {
                    hub.server.register($.connection.hub.id, @user.account.account_id, Cookies.get('language_id'));
                });
                $.connection.hub.disconnected(function () {
                    setTimeout(function () {
                        $.connection.hub.start();
                    }, 5000); // Restart connection after 5 seconds.
                });

                $.ajax({
                    url: '/Notification/List',
                    data: {
                        start: 0
                    },
                    dataType: "json",
                    success: function (data) {
                        if (data.success) {
                            for (var i = 0; i < data.content.length; i++) {
                                let Noti = data.content[i];
                                let templateMessage = '<a data-id="' + Noti.NotificationID + '" href="' + Noti.URL + '" class="navi-item';
                                if (!Noti.IsRead)
                                    templateMessage += ' not-read-noti';
                                templateMessage += '"><div class="navi-link">' +
                                    '<div class="navi-icon mr-2">' +
                                    '<i class="' + Noti.Icon + '"></i>' +
                                    '</div>' +
                                    '<div class="navi-text">' +
                                    '<div';
                                if (Noti.IsRead)
                                    templateMessage += ' class="text-muted"'
                                templateMessage += '>' +
                                                Noti.Template +
                                            '</div>' +
                                            '<div class="text-muted">' +
                                                Noti.StringDate +
                                            '</div>' +
                                        '</div>' +
                                    '</div>' +
                                    '</a>';
                                $("#event-noti").append(templateMessage);
                            }
                            $("#noti-number span").text(data.unread)
                            if (data.unread != 0) {
                                $("#noti-number").show();
                                $("#noti-pulse").show();
                            }
                            if ($("#event-noti a").length == 0)
                                $("#event-noti").append('<div style="height: inherit" class="d-flex flex-center text-center text-muted">All caught up!<br />No new notifications.</div>')
                        }
                    },
                    error: function (data) {
                        toastr.error("Không thể lấy được dữ liệu thông báo");
                    }
                });

                $(document).on("click", ".not-read-noti", function (e) {
                    location.href = "/Notification/Read?id=" + $(this).data("id");
                    return false;
                })
            })
        </script>
    }
</body>
<!--end::Body-->

</html>