    <script>
        var signOut = new LoaderBtn($("#signOut"));
        $(".change-language").click(function () {
            document.cookie = "language_name=" + $(this).data("language") + '; Path=/;';
            document.cookie = "language_id=" + $(this).data("id") + '; Path=/;';
            location.reload();
        });
        function onSignIn(googleUser) {
            var id_token = googleUser.getAuthResponse().id_token;
            //var xhr = new XMLHttpRequest();
            //xhr.open('POST', );
            //xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
            //xhr.onreadystatechange = function () {
            //    if (this.readyState == 4 && this.status == 200) {
            //        console.log(this);
            //    }
            //};
            //xhr.send();
            let fd = new FormData();
            fd.append("idtoken", id_token);
            $.ajax({
                url: '/Authen/SigninGoogleAsync',
                data: fd,
                type: "POST",
                success: function (data) {
                    if (data.success == false) {
                        toastr.warning(data.content);
                        var auth2 = gapi.auth2.getAuthInstance();
                        auth2.signOut().then(function () {
                        });
                    } else if (data.redirect == true) {
                        window.location = "/AdditionalProfile";
                    } else {
                        window.location.reload();
                    }
                },
                error: function (data) {
                    toastr.error("Có lỗi xảy ra")
                },
                processData: false,
                contentType: false,
                cache: true
            });
        }
        function gp_signOut() {
            signOut.startLoading()
            $.ajax({
                url: '/Authen/Logout',
                data: {},
                type: "POST",
                contentType: "application/json;charset=utf-8",
                success: function (data) {
                    var auth2 = gapi.auth2.getAuthInstance();
                    auth2.signOut().then(function () {
                        window.location.href = '/';
                    });
                    signOut.stopLoading()
                },
                error: function (data) {
                    alert(data)
                }
            });
        }
        $("#login_btn").click(function () {
            $("#google_auth_login").find(".abcRioButtonContentWrapper").click();
        });
        function onLoad() {
            gapi.load('auth2', function () {
                gapi.auth2.init();
            });
        }
        $(document).ready(function () {
            onLoad();
        });
    </script>
