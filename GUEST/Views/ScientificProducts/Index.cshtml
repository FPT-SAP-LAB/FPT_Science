@section scripts{
    <script src="~/Scripts/ListAllPaper.js"></script>
    @*<script src="~/Content/assets/js/pages/crud/ktdatatable/base/html-table.js"></script>*@
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.23/css/jquery.dataTables.css">
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.23/js/jquery.dataTables.js"></script>
    <script>
        $(document).ready(function () {
            $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
                $.fn.dataTable.tables({ visible: true, api: true }).columns.adjust();
            });
            $('#table_tapchi').DataTable({
                "columns": [
                    { width: "5%" },
                    { width: "20%" },
                    { width: "20%" },
                    { width: "20%" },
                    { width: "5%" },
                    { width: "10%" },
                    { width: "10%" },
                    {
                        width: "5%",
                        render: function () {
                            return "<a href='#'>link</a>"
                        }
                    },
                    {
                        width: "5%",
                        render: function () {
                            return "<a href='#'>link</a>"
                        }
                    }
                ],
                oLanguage: {
                    oPaginate: {
                        sPrevious: "@rm.GetString("Pre")",
                        sNext: "@rm.GetString("Next")"
                    },
                    sEmptyTable: "@rm.GetString("EmptyTB")",
                    sInfo: "@rm.GetString("ShowTB")",
                    sLengthMenu: "@rm.GetString("Show") <select>" +
                        "<option value='5'>5</option>" +
                        "<option value='10'>10</option>" +
                        "<option value='20'>20</option>" +
                        "</select> @rm.GetString("Record")",
                    sSearch: "@rm.GetString("Search")"
                }
            });

            $('#table_kyyeu').DataTable({
                "columns": [
                    { width: "5%" },
                    { width: "20%" },
                    { width: "20%" },
                    { width: "20%" },
                    { width: "15%" },
                    {
                        width: "15%"
                    },
                    {
                        width: "5%",
                        render: function () {
                            return "<a href='#'>link</a>"
                        }
                    }
                ],
                oLanguage: {
                    oPaginate: {
                        sPrevious: "@rm.GetString("Pre")",
                        sNext: "@rm.GetString("Next")"
                    },
                    sEmptyTable: "@rm.GetString("EmptyTB")",
                    sInfo: "@rm.GetString("ShowTB")",
                    sLengthMenu: "@rm.GetString("Show") <select>" +
                        "<option value='5'>5</option>" +
                        "<option value='10'>10</option>" +
                        "<option value='20'>20</option>" +
                        "</select> @rm.GetString("Record")",
                    sSearch: "@rm.GetString("Search")"
                }
            });
        });
    </script>
